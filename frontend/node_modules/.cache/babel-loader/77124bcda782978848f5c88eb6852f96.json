:["DetailView.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row>\n      <v-col cols=\"12\" sm=\"8\">\n        <h1 class=\"display-3 fst-italic\">{{book.name}}</h1>\n        <v-row class=\"no-gutter\">\n          <h1 class=\"text-h5 fw-bold\">Authors:</h1>\n          <span class=\"gray--text\" v-for=\"author in book.authors\" :key=\"author\">\n            {{ author }}\n          </span>\n        </v-row>\n        <hr>\n        <div class=\" flex flex-column\">\n           <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"mr-2 fw-semibold fs-5\">RELEASED DATE:</span>\n             <span class=\"lead\">{{book.released | moment(\"dddd, MMMM Do YYYY\") }}</span>\n           </div>\n            <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"mr-2 fw-semibold fs-5\">TOTAL PAGES:</span>\n             <span class=\"lead\">{{book.numberOfPages}}</span>\n           </div>\n           <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"mr-2 fw-semibold fs-5\">ISBN:</span>\n             <span class=\"lead\">{{book.isbn}}</span>\n           </div>\n           <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"lead mr-2 fw-semibold\">PUBLISHER:</span>\n             <span class=\"lead\">{{book.publisher}}</span>\n           </div>\n           <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"lead mr-2 fw-semibold\">MEDIATYPE:</span>\n             <span class=\"lead\">{{book.mediaType}}</span>\n           </div>\n           <div class=\"d-flex flex-row justify-content-start align-items-center\">\n             <span class=\"lead mr-2 fw-semibold\">COUNTRY:</span>\n             <span class=\"lead\">{{book.country}}</span>\n           </div>\n        </div>\n        <hr>\n        <sorted-table :values=\"values\">\n          <thead>\n            <tr>\n              <th scope=\"col\" style=\"text-align: left; width: 10rem;\">\n                <sort-link name=\"id\">Name</sort-link>\n              </th>\n              <th scope=\"col\" style=\"text-align: left; width: 10rem;\">\n                <sort-link name=\"name\">Gender</sort-link>\n              </th>\n              <th scope=\"col\" style=\"text-align: left; width: 10rem;\">\n                <sort-link name=\"hits\">Age</sort-link>\n              </th>\n            </tr>\n          </thead>\n          <template #body=\"{ values }\">\n            <tbody>\n              <tr v-for=\"value in values\" :key=\"value.name\">\n              <td>{{ value.name }}</td>\n              <td>{{ value.gender }}</td>\n              <td>{{ value.age }}</td>\n            </tr>\n            </tbody>\n          </template>\n    </sorted-table>\n        <div class=\"mt-5\">\n          <h2 class=\"blog-post-title mb-1\">Characters:</h2>\n          <div :key=\"index\" v-for=\"(crew, index) in characters\" class=\"mt-5\">\n            {{ fetchCharacters(crew) }}\n          </div>\n        </div>\n      </v-col>\n    </v-row>\n  </v-container>      \n</template>\n\n<script>\nimport { Component, Vue } from 'vue-property-decorator'\n\n@Component({\n  data: () => ({\n    book: {},\n    characters: [],\n    povCharacters: [],\n    values: [\n        { name: \"Plugin Foo\", gender: 2, age: 33 },\n        { name: \"Plugin Bar\", gender: 1, age: 42 },\n        { name: \"Plugin Foo Bar\", gender: 3, age: 79 }\n      ]\n  }),\n  methods: {\n    async fetchCharacters(charactersArray) {\n      await charactersArray.forEach(async (character) => {\n        const response = await this.$http.get(character);\n        console.log('CHAR URL', response)\n      })\n      // const response = await this.$http.get(url);\n      // console.log('CHAR URL', response)\n    },\n  },\n  async created(){\n    try {\n      this.loading = true\n      const params = this.$route.params.id\n      const response = await this.$http.get(`/books/get-book/?bookId=${params}`)\n      this.book = await response.data.response\n      this.characters = await this.fetchCharacters(this.book.characters)\n      this.povCharacters = await response.data.response.povCharacters\n      this.loading = false\n      console.log('Book', this.book)\n    }catch (error) {\n      console.log(error)\n    }\n  },\n  \n})\nexport default class DetailView extends Vue {}\n</script>\n<style>\n  @import'~bootstrap/dist/css/bootstrap.css';\n</style>"]},"metadata":{},"sourceType":"module"}